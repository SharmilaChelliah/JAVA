import java.util.*;
public class Alpha{
public static void main(String args[]){
char input1,input2;
Scanner sc = new Scanner(System.in);
input1 = sc.next().charAt(0);
input2 = sc.next().charAt(0);
if(input1>input2)
{
System.out.println(input2+","+input1);
}
else
{
System.out.println(input1+","+input2);
}
}
}
